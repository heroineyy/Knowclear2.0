.weui-search-bar {
  position: relative;
  padding: 20px 5px 0px 5px ;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  box-sizing: border-box;
  border-top: 1rpx;
  border-bottom: 1rpx ;
}
.weui-icon-search_in-box {
  position: absolute;
  right: 5px;
  top: 1px;
}
.weui-search-bar__form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
          flex: auto;
  border-radius: 30px;
  background: #FFFFFF;
  border: 1rpx solid #E6E6EA;
}
.weui-search-bar__box {
  position: relative;
  padding-left: 15px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
  border-radius: 2px;
}
.weui-search-bar__input {
  height: 25px;
  line-height: 28px;
  font-size: 13px;
}
.weui-search-bar__cancel-btn {
  margin-left: 10px;
  line-height: 28px;
  color: #09BB07;
  white-space: nowrap;
}
